{
  "name": "RECP",
  "version": "0.0.1",
  "description": "Personal recipe app",
  "main": "index.js",
  "repository": "https://github.com/yannickbuntsma/recp",
  "author": "yannickbuntsma <yannickbuntsma@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf .next dist",
    "dev": "next -p 8000",
    "build": "next build",
    "start": "next start -p 8000",
    "fix": "yarn fix:js",
    "fix:js": "prettier --write ./**/*.{ts,tsx} --parser typescript",
    "lint": "tslint --project .",
    "lint:types": "tsc --noEmit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w --jsx preserve",
    "watch-node": "nodemon dist/server.js",
    "serve": "concurrently \"yarn watch-ts\" \"yarn watch-node\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commmit": "yarn fix"
    }
  },
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "@types/express": "^4.16.1",
    "@types/react-redux": "^7.0.5",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.18.0",
    "babel-plugin-emotion": "^10.0.9",
    "contentful": "^7.5.0",
    "dotenv": "^7.0.0",
    "emotion-theming": "^10.0.10",
    "envalid": "^4.2.2",
    "express": "^4.16.4",
    "next": "^8.0.3",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "reselect": "^4.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/next": "^8.0.3",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.11",
    "ts-jest": "^24.0.2",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  }
}
